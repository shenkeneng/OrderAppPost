<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 选中时的图片背景 -->
    <item android:color="@color/frxs_red" android:state_checked="true" />
    <!-- 单击时的背景图片 -->
    <item android:color="@color/frxs_red" android:state_pressed="true" />
    <item android:color="@color/frxs_red" android:state_focused="true" />
    <item android:color="@color/frxs_red" android:state_selected="true" />
    <!-- 默认时的背景图片 -->
    <item android:color="@color/frxs_black_dark"></item>

</selector>